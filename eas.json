{
  "cli": {
    "version": ">= 16.19.1",
    "appVersionSource": "remote"
  },
"build": {
     "staging": {
      "developmentClient": true,
      "distribution": "internal",
      "android": { "buildType": "apk" },
      "ios": { "simulator": true },
      "env" : {
        "API_URL": "https://api.smartmath.pl/api/v1"
      }
    },
     "preview": {
      "distribution": "internal",
      "channel": "preview",
        "android": {
        "buildType": "apk"
      },
      "env" : {
        "API_URL": "https://api.smartmath.pl/api/v1"
      }
    },
    "production": {
      "developmentClient": false,
      "distribution": "store",
      "android": { "buildType": "app-bundle" },
      "ios": { "simulator": false },
      "env" : {
        "API_URL": "https://api.smartmath.pl/api/v1"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
