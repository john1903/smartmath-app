
{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "react-native",
    "strict": true,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "allowJs": true,
    "checkJs": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "noEmit": true
  },
   "include": [
    "src",
    "app.config.ts",
    "**/*.ts",
    "**/*.tsx",
    "**/*.js",
    "declarations.d.ts"
  ],
  "exclude": ["node_modules"],
  "extends": "expo/tsconfig.base"
}
